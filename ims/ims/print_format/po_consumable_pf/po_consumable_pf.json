idth:50%;\" class=\"text-center\"><img src=\"\" height = \"60\" width = \"95\" alt=\"no signature\"><br>{{row.emp_name}}<br>{{row.designation}}<br>Date of Approval: {{row.date_of_approval}}</td>-->\n<!--            {%- endif -%}-->\n<!--        {%- endif -%}-->\n<!--    {%- endfor -%}-->\n<!--    {%- for row in doc.authorized_signature-%}-->\n<!--        {%- if row.designation == \"VC, School of Medicine & PBMH, KIMS\" -%}-->\n<!--            {%- if frappe.get_all(\"Employee\",{\"name\":row.emp_id},[\"signature\"])[0][\"signature\"] != None -%}-->\n<!--                <td style=\"width:50%;\" class=\"text-center\"><img src=\"{{frappe.get_all(\"Employee\",{\"name\":row.emp_id},[\"signature\"])[0][\"signature\"]}}\" width=\"60\" height=\"100\" alt=\"no signature\"><br>{{row.emp_name}}<br>{{row.designation}}<br>Date of Approval: {{row.date_of_approval}}</td>-->\n<!--            {%- else -%}-->\n<!--                <td style=\"width:50%;\" class=\"text-center\"><img src=\"\" height = \"60\" width = \"95\" alt=\"no signature\"><br>{{row.emp_name}}<br>{{row.designation}}<br>Date of Approval: {{row.date_of_approval}}</td>-->\n<!--            {%- endif -%}-->\n<!--        {%- endif -%}-->\n<!--    {%- endfor -%}-->\n<!--    </tr>-->\n<!--    </tbody>-->\n<!--</table>-->\n<!--<table style=\"line-height: 1em;width: 100%;font-size: 0.8em;\" border=\"1\" cellpadding=\"1\" cellspacing=\"1\">-->\n<!--    <tbody>-->\n<!--    <tr><td style=\"width:50%;\">Remarks by Director Administrator: <br><br>-->\n<!--    {%- for row in doc.authorized_signature-%}-->\n    \n<!--    {%- if row.designation == \"Director Administration\" and row.remarks != None -%}-->\n<!--    {{row.emp_name}}: &nbsp;-->\n<!--    {{row.remarks}}<br><br>-->\n<!--    {%- endif -%}-->\n<!--    {%- endfor -%}</td>-->\n<!--    <td style=\"width:50%;\">Remarks by VC, School of Medicine & PBMH, KIMS: <br><br>-->\n<!--    {%- for row in doc.authorized_signature-%}-->\n    \n<!--    {%- if row.designation == \"VC, School of Medicine & PBMH, KIMS\" and row.remarks != None -%}-->\n<!--    {{row.emp_name}}: &nbsp;-->\n<!--    {{row.remarks}}<br><br>-->\n<!--    {%- endif -%}-->\n<!--    {%- endfor -%}</td></tr></tbody>-->\n<!--</table>-->\n\n<br>\n\n{% set workflow_doc_name = frappe.get_all(\"Workflow\",{\"document_type\":\"PO Consumable\",\"is_active\":1},['name'])[0]['name'] %}\n{% set workflow_name=frappe.get_all(\"Role Permission Tool\",{\"workflow_name\":workflow_doc_name})[0]['name'] %}\n{% set grouping_of_deg_name=frappe.db.sql(\"\"\" select grouping_of_designation, single_user,COUNT(grouping_of_designation)  \nfrom `tabRole Permission Tool Child`\nwhere parent=\"\"\"+workflow_name|string+\"\"\"\nGROUP BY grouping_of_designation HAVING COUNT(grouping_of_designation) > 1 \"\"\") %}\n\n{%for t in grouping_of_deg_name %}\n    {% set group_name=t[0]%}\n    <table style=\"line-height: 1em;width: 100%;font-size: 0.8em;\" border=\"1\" cellpadding=\"1\" cellspacing=\"1\">\n        <tbody>\n        <tr>\n            \n            <td class=\"text-center\"><b>{{frappe.get_all(\"Section Head Name\",{\"parent\":workflow_name,\"lable_name\":group_name},[\"name_of_the_group\"])[0]['name_of_the_group']}}</b></td>\n        </tr>\n        </tbody>\n    </table>\n    <table style=\"line-height: 1em;width: 100%;font-size: 0.8em;\" border=\"1\" cellpadding=\"1\" cellspacing=\"1\">\n        <tbody>\n        <tr>\n                {% for row in doc.authorized_signature%}\n                    {% if row.grouping_of_designation==group_name and row.approval_status!=\"Draft\" and row.approval_status!=\"Verify and Save\"%}\n                        {%- if frappe.get_all(\"Employee\",{\"name\":row.emp_id},[\"signature\"])[0][\"signature\"] != None -%}\n                            <td style=\"width:50%;\" class=\"text-center\"><img src=\"{{frappe.get_all(\"Employee\",{\"name\":row.emp_id},[\"signature\"])[0][\"signature\"]}}\" height = 60 width = 95 alt=\"no signature\"><br>{{row.emp_name}}<br>{{row.designation}}<br>Date of Approval: {{row.date_of_approval}}</td>\n                        {%- else -%}\n                            <td style=\"width:50%;\" class=\"text-center\"><img src=\"\" height = \"60\" width = \"95\" alt=\"no signature\"><br>{{row.emp_name}}<br>{{row.designation}}<br>Date of Approval: {{row.date_of_approval}}</td>\n                        {% endif %}\n                    {%endif%}\n                {% endfor %}\n          \n                {%if flag==\"No\"%}\n                    <td class=\"text-center\">{{j['designation']}}</td>\n                {%endif%}\n            \n        </tr>\n        </tbody>\n    </table>\n    <table style=\"line-height: 1em;width: 100%;font-size: 0.8em;\" border=\"1\" cellpadding=\"1\" cellspacing=\"1\">\n        <tbody>\n            {%- for row in doc.authorized_signature-%}\n                {% if row.grouping_of_designation==group_name and row.remarks != None and row.remarks !=\"\" %}\n                 <br><br>\n                    <tr><td>Remarks by :<br><br>\n                        {{row.emp_name}}: &nbsp;\n                        {{row.remarks}}<br><br>\n                {% endif %}    \n            {% endfor %}\n        </tr></tbody>\n    </table>\n\n{%endfor%}\n\n{% set single_name=frappe.db.sql(\"\"\" select designation,grouping_of_designation, single_user  \nfrom `tabRole Permission Tool Child`\nwhere single_user=1 and parent=\"\"\"+workflow_name|string) %}\n    {%for t in single_name%}\n    \n        <table style=\"line-height: 1em;width: 100%;font-size: 0.8em;\" border=\"1\" cellpadding=\"1\" cellspacing=\"1\">\n            <tbody>\n            <tr>{%- for row in doc.authorized_signature-%}\n                {%- if row.designation ==t[0] -%}\n                    {%- if frappe.get_all(\"Employee\",{\"name\":row.emp_id},[\"signature\"])[0][\"signature\"] != None -%}\n                        <td class=\"text-center\"><img src=\"{{frappe.get_all(\"Employee\",{\"name\":row.emp_id},[\"signature\"])[0][\"signature\"]}}\" width=\"60\" height=\"100\" alt=\"no signature\"><br>{{row.emp_name}}<br>{{row.designation}}<br>Date of Approval: {{row.date_of_approval}}</td>\n                    {%- else -%}\n                        <td class=\"text-center\"><img src=\"\" height = 60 width = 95 alt=\"no signature\"><br>{{row.emp_name}}<br>{{row.designation}}<br>Date of Approval: {{row.date_of_approval}}</td>\n                    {%- endif -%}\n                {%- endif -%}\n            {%- endfor -%}</tr>\n            </tbody>\n        </table>\n        <table style=\"line-height: 1em;width: 100%;font-size: 0.8em;\" border=\"1\" cellpadding=\"1\" cellspacing=\"1\">\n            <tbody>\n            <tr><td>Remarks by {{t[0]}} <br><br>\n            {%- for row in doc.authorized_signature-%}\n            \n            {%- if row.designation == t[0] and row.remarks != None and row.remarks !=\"\" -%}\n            {{row.emp_name}}: &nbsp;\n            {{row.remarks}}<br><br>\n            {%- endif -%}\n            {%- endfor -%}</td></tr></tbody>\n        </table>\n        <table style=\"line-height: 1em;width: 100%;font-size: 0.8em;\" border=\"1\" cellpadding=\"1\" cellspacing=\"1\">\n    {%endfor%}     \n</html>",
