erarchy -%}-->\n<!--            {%- if row.designation == \"Accounts - KIMS\" and loop.index==1 -%}-->\n<!--                {%- if frappe.get_all(\"Employee\",{\"name\":row.emp_id},[\"signature\"])[0][\"signature\"] != None -%}-->\n<!--                    <td style=\"width:25%;\" class=\"text-center\"><img src=\"{{frappe.get_all(\"Employee\",{\"name\":row.emp_id},[\"signature\"])[0][\"signature\"]}}\" width=\"60\" height=\"100\" alt=\"no signature\" ><br>{{row.emp_name}}<br>{{row.designation}}<br>Date of Approval: {{row.get_formatted(\"date_of_approval\")}}</td>-->\n<!--                {%- endif -%}-->\n<!--            {%- endif -%}-->\n<!--        {%- endfor -%}-->\n<!--        </table>-->\n<!--</div>-->\n\n\n\n\n\n<!--<p style=\"page-break-before: always;\">-->\n<table style=\"line-height: 0.5em;width: 100%;\" border=\"0\" cellpadding=\"2\" cellspacing=\"2\">\n<tbody>\n<tr>\n    <td><br><center><img src=\"/files/logo-removebg-preview.png\" alt=\"KIIT\" width=\"80\" height=\"70\"><center>\n    <td><p style=\"text-align: center; font-size: 1em; letter-spacing: 0.7px;\"><b>KALINGA INSTITUTE OF MEDICAL SCIENCES</b><br><br>\n    <span style=\"text-align: center; font-size: 1em;\">Kushabhadra Campus - 05<br><br></span>\n<span style=\"text-align: center; font-size: 1em;\">Patia, Bhubaneswar, Odisha - 751024<br></span>\n</p></td>\n<td><center><img src=\"/files/kims.png\" alt=\"KIMS\" width=\"100\" height=\"100\"></center></td>\n</tr>\n</tbody></table>\n<p style=\"text-align: right;\"><b>Ref</b>{{doc.get_formatted(\"audit_reference_no\")}}</p>\n<p style=\"text-align: right;\"><b>Date</b>{{doc.get_formatted(\"audit_posting_date\")}}</p>\n<p>To</p>\n<p>The Branch Manager,</p>\n<p>{{doc.bank_name}},</p>\n<p>{{doc.branch_details}}</p>\n\n<p><b>Sub:-</b>Remittance of Fund through RTGS</p>\n<p>Dear sir,</p>\n<p>Please Transfer Rs. {{doc.total_amount}}/-({{ frappe.utils.money_in_words(doc.total_amount) }}) only from SB A/C No. {{doc.bank_ac_no}} against Cq no....{{doc.cheque_no}}.... dated ....{{doc.get_formatted(\"cheque_date\")}}.... bank charges may be debited to our account. Details as below:-</p>\n<table style=\"line-height: 1em;width: 100%;font-size: 0.8em;\" border=\"1\" cellpadding=\"1\" cellspacing=\"1\">\n    <thead>\n        <tr>\n            <th class=\"text-center\" style=\"width:5%; font-color: black;\">SL</th>\n            <th class=\"text-center\" style=\"width:19%;\">{{_(\"A/C Holder Name\")}}</th>\n            <th class=\"text-center\" style=\"width:24%;\">{{_(\"Bank Name\")}}</th>\n            <th class=\"text-center\" style=\"width:26%;\">{{_(\"A/C No\")}}</th>\n            <th class=\"text-center\" style=\"width:25%;\">{{_(\"IFSC Code\")}}</th>\n            <th class=\"text-center\" style=\"width:25%;\">{{_(\"Branch\")}}</th>\n            <th class=\"text-center\" style=\"width:25%;\">{{_(\"Amt. (Rs)\")}}</th>\n            <th class=\"text-center\" style=\"width:25%;\">{{_(\"UTR No\")}}</th>\n        </tr>\n    </thead>\n    <tbody>\n        <tr>{%- for row in doc.vendor_wise_payment_details-%}\n            <td class=\"text-center\">{{ loop.index }}</td>\n            <td class=\"text-center\">{{ row.get_formatted(\"ac_holder_name\") }}</td>\n            <td class=\"text-center\">{{ row.get_formatted(\"bank_name\")}}</td>\n            <td class=\"text-right\">{{ row.get_formatted(\"ac_no\") }}</td>\n            <td class=\"text-center\">{{ row.ifsc_code }}</td>\n            <td class=\"text-center\">{{ row.branch }}</td>\n            <td class=\"text-center\">{{ row.amount }}</td>\n            {%- if row.mode_of_payment==\"NEFT\" or row.mode_of_payment==\"RTGS\" or row.mode_of_payment==\"IMPS\" -%}\n            <td class=\"text-center\">{{ row.get_formatted(\"utr_number\")}}</td>\n            {%- endif -%}\n            {%- if row.mode_of_payment==\"Cheque\"-%}\n            <td class=\"text-center\">{{ row.get_formatted(\"cheque_no\")}}</td>\n            {%- endif -%}\n            {%- if row.mode_of_payment==\"Cash\"-%}\n            <td class=\"text-center\">{{ row.get_formatted(\"cash_receipt\")}}</td>\n            {%- endif -%}\n        </tr>{%- endfor -%}\n    </tbody>\n    <table style=\"line-height: 1em;width: 100%;font-size: 0.8em;\" border=\"1\" cellpadding=\"1\" cellspacing=\"1\">\n    <tr>\n        <th style=\"text-align:right;\">Total:- {{doc.get_formatted(\"total_amount\")}}</th>\n    </tr>\n</table>\n<p><center>{{frappe.utils.money_in_words(doc.total_amount)}}</center>\n<p>Email & Mobile No:- {{doc.get_formatted(\"contact_email_id\")}} (Mob:-{{doc.get_formatted(\"contact_phone_no\")}})</p>\n<br>\n<br>\n<br>\n<p>Thanking You,</p>\n<p>Yours Faithfully</p>\n<p>For PRADYUMNA BAL MEMORIAL HOSPITAL</p>\n<br>\n<br>\n<br>\n<br>\n<p>Authorized Signatory</p>\n",
